from Tkinter import *
import webbrowser

root = Tk()

def weather():
	webbrowser.open('https://www.twojapogoda.pl/')
def info():
	webbrowser.open('https://')
pogoda = Button(root, text="Pogoda", command=weather, padx=50, pady=50)
wiadomosci = Button(root, text="Wloszczowa", command=info, padx=50, pady=50)
pogoda.grid(row=3,column=0)
wiadomosci.grid(row=3,column=1)

root.mainloop()
